{
    "name": "vue-brush",
    "version": "1.0.0",
    "description": "vue-brush",
    "private": true,
    "author": "",
    "scripts": {
        "serve": "SET LOCAL_DEBUG=true & vue-cli-service serve --open --mode development",
        "serve:uat": "SET LOCAL_DEBUG=true & vue-cli-service serve --open --mode local.uat",
        "serve:prod": "SET LOCAL_DEBUG=false & vue-cli-service serve --open --mode local.prod",
        "serve:mock": "SET LOCAL_DEBUG=false & vue-cli-service serve --open --mode mockjs",
        "serve:mac": "export LOCAL_DEBUG=true && vue-cli-service serve --open --mode development",
        "serve:mac-mock": "export LOCAL_DEBUG=true && vue-cli-service serve --open --mode mockjs",
        "build": "vue-cli-service build",
        "build:uat": "vue-cli-service build --mode uat",
        "build:test": "vue-cli-service build --mode test",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "bootstrap-vue": "^2.17.3",
        "compression-webpack-plugin": "^5.0.1",
        "core-js": "^3.6.5",
        "jshint": "^2.12.0",
        "swiper": "^6.5.7",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "vue": "^2.6.11",
        "vue-router": "^3.5.1",
        "vuelidate": "^0.7.6"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.11.5",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "babel-eslint": "^10.1.0",
        "bootstrap": "^4.5.2",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "filemanager-webpack-plugin": "^6.1.4",
        "mockjs": "^1.1.0",
        "mutationobserver-shim": "^0.3.7",
        "popper.js": "^1.16.1",
        "portal-vue": "^2.1.7",
        "sass": "^1.26.11",
        "sass-loader": "^10.0.2",
        "vue-awesome-swiper": "^3.1.3",
        "vue-cli-plugin-bootstrap-vue": "~0.7.0",
        "vue-cli-plugin-mock": "^1.0.3",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}